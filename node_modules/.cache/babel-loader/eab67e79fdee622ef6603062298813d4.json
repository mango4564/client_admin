\">\r\n\t\t\t\t\t\t\t<el-option v-for=\"o in list_user_housekeeping_personnel\" :key=\"o['username']\" :label=\"o['nickname'] + '-' + o['username']\"\r\n\t\t\t\t\t\t\t\t\t   :value=\"o['user_id']\">\r\n\t\t\t\t\t\t\t</el-option>\r\n\t\t\t\t\t\t</el-select>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t</el-col>\r\n\t\t\t<el-col v-if=\"user_group === '管理员' || $check_field('get','remarks') || $check_field('add','remarks') || $check_field('set','remarks')\" :xs=\"24\" :sm=\"12\" :lg=\"8\">\r\n\t\t\t\t<el-form-item label=\"备注\" prop=\"remarks\">\r\n\t\t\t\t\t<el-input type=\"textarea\" id=\"remarks\" v-model=\"form['remarks']\" placeholder=\"请输入备注\"\r\n\t\t\t\t\t\tv-if=\"user_group === '管理员' || (form['dormitory_information_id'] && $check_field('set','remarks')) || (!form['dormitory_information_id'] && $check_field('add','remarks'))\" :disabled=\"disabledObj['remarks_isDisabled']\"></el-input>\r\n\t\t\t\t\t<div v-else-if=\"$check_field('get','remarks')\">{{form['remarks']}}</div>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t</el-col>\r\n\t\t\t<el-col :xs=\"24\" :sm=\"12\" :lg=\"8\">\r\n\t\t\t\t<el-form-item>\r\n\t\t\t\t\t<el-button type=\"primary\" @click=\"submit()\">提交</el-button>\r\n\t\t\t\t\t<el-button @click=\"cancel()\">取消</el-button>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t</el-col>\r\n\r\n\t\t</el-form>\r\n\t</el-main>\r\n</template>\r\n\r\n<script>\r\n\timport mixin from \"@/mixins/page.js\";\r\n\r\n\texport default {\r\n\t\tmixins: [mixin],\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\tfield: \"dormitory_information_id\",\r\n\t\t\t\turl_add: \"~/api/dormitory_information/add?\",\r\n\t\t\t\turl_set: \"~/api/dormitory_information/set?\",\r\n\t\t\t\turl_get_obj: \"~/api/dormitory_information/get_obj?\",\r\n\t\t\t\turl_upload: \"~/api/dormitory_information/upload?\",\r\n\r\n\t\t\t\tquery: {\r\n\t\t\t\t\t\"dormitory_information_id\": 0,\r\n\t\t\t\t},\r\n\r\n\t\t\t\tform: {\r\n\t\t\t\t\t\"building\":'', // 楼栋\r\n\t\t\t\t\t\"floor\":'', // 楼层\r\n\t\t\t\t\t\"room_number\":'', // 房号\r\n\t\t\t\t\t\"room_type\":'', // 房间类型\r\n\t\t\t\t\t\"number_of_beds\":0, // 床位数\r\n\t\t\t\t\t\"housekeeping_personnel\": 0, // 宿管人员\r\n\t\t\t\t\t\"remarks\":'', // 备注\r\n\t\t\t\t\t\"dormitory_information_id\": 0, // ID\r\n\r\n\t\t\t\t},\r\n\t\t\t\tdisabledObj:{\r\n\t\t\t\t\t\"building_isDisabled\": false,\r\n\t\t\t\t\t\"floor_isDisabled\": false,\r\n\t\t\t\t\t\"room_number_isDisabled\": false,\r\n\t\t\t\t\t\"room_type_isDisabled\": false,\r\n\t\t\t\t\t\"housekeeping_personnel_isDisabled\": false,\r\n\t\t\t\t\t\"remarks_isDisabled\": false,\r\n\t\t\t\t},\r\n\t\t\t\t// 用户列表\r\n\t\t\t\tlist_user_housekeeping_personnel: [],\r\n\t\t\t\t// 用户组\r\n\t\t\t\tgroup_user_housekeeping_personnel: \"\",\r\n\r\n\t\t\t}\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\t/**\r\n\t\t\t * 获取宿管人员用户列表\r\n\t\t\t */\r\n\t\t\tasync get_list_user_housekeeping_personnel() {\r\n                // if(this.user_group !== \"管理员\" && this.form[\"housekeeping_personnel\"] === 0) {\r\n                //     this.form[\"housekeeping_personnel\"] = this.user.user_id;\r\n                // }\r\n                var json = await this.$get(\"~/api/user/get_list?user_group=宿管人员\");\r\n                if(json.result && json.result.list){\r\n                    this.list_user_housekeeping_personnel = json.result.list;\r\n                }\r\n                else if(json.error){\r\n                    console.error(json.error);\r\n                }\r\n\t\t\t},\r\n\t\t\t/**\r\n\t\t\t * 获取宿管人员用户组\r\n\t\t\t */\r\n\t\t\tasync get_group_user_housekeeping_personnel() {\r\n\t\t\t\tthis.form[\"housekeeping_personnel\"] = this.user.user_id;\r\n\t\t\t\tvar json = await this.$get(\"~/api/user_group/get_obj?name=宿管人员\");\r\n\t\t\t\tif(json.result && json.result.obj){\r\n\t\t\t\t\tthis.group_user_housekeeping_personnel = json.result.obj;\r\n\t\t\t\t}\r\n\t\t\t\telse if(json.error){\r\n\t\t\t\t\tconsole.error(json.error);\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\tget_user_session_housekeeping_personnel(id){\r\n\t\t\t\tvar _this = this;\r\n\t\t\t\tvar us